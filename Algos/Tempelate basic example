#include<bits/stdc++.h>
using namespace std;
template<class x>void swapargs (x&a,x&b)
{
    x swap;
    swap=a;
    a=b;
    b=swap;
}
int main()
{
    cout<<"Enter the values of the two variables to be swapped\n.";
    string s1,s2;
    getline(cin,s1);
    getline(cin,s2);
    swapargs(s1,s2);
    cout<<endl<<"The values after swapping is\n";
    cout<<s1<<endl;
    cout<<s2<<endl;
    return 0;
}

 
